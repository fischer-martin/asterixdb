-- JEDI = 3: rename d1 to d2, delete [], and delete "txt2".
SELECT VALUE jedi(
    (SELECT parse_json('{"id": 7, "name": "Jerry", "comment": "txt"}') d1),
    (SELECT parse_json('{"id": 7, "name": "Jerry", "comment": ["txt", "txt2"]}') d2)
);